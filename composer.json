{
  "name": "boldtrn/jsonb-bundle",
  "license": "MIT",
  "type": "library",
  "description": "A library adding the support for JSONB columns of PostgreSQL to Doctrine.",
  "keywords": [
    "jsonb",
    "doctrine",
    "symfony",
    "bundle",
    "postgresql"
  ],
  "homepage": "https://github.com/boldtrn/JsonbBundle",
  "autoload": {
    "psr-4": {
      "Boldtrn\\JsonbBundle\\": ""
    }
  },
  "authors": [
    {
      "name": "Robin Boldt",
      "email": "boldtrn@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.3.3",
    "doctrine/dbal": "~2.4"
  },
  "suggest": {
    "doctrine/orm": "To use DQL functions"
  },
  "require-dev": {
    "doctrine/orm": ">2.4,<2.6",
    "phpunit/phpunit": "~4.2"
  }
}
